<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="3.20"/>
    <requires lib="libhandy" version="0.0"/>
    <template class="DungeonJournalCharacterStatsView" parent="DungeonJournalCharacterView">
        <property name="visible">True</property>
        <child internal-child="box">
            <object class="GtkBox">
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="proficiency_bonus_character_entry">
                        <property name="entry_name" translatable="yes">Proficiency Bonus</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="armor_class_character_entry">
                        <property name="entry_name" translatable="yes">Armor Class</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="initiative_character_entry">
                        <property name="entry_name" translatable="yes">Initiative</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="speed_character_entry">
                        <property name="entry_name" translatable="yes">Speed</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="hp_max_character_entry">
                        <property name="entry_name" translatable="yes">Hit Point Maximum</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="hp_character_entry">
                        <property name="entry_name" translatable="yes">Current Hit Points</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntryAdjustment" id="hp_temp_character_entry">
                        <property name="entry_name" translatable="yes">Temporary Hit Points</property>
                    </object>
                </child>
                <child>
                    <object class="DungeonJournalCharacterEntry" id="hit_dice_character_entry">
                        <property name="entry_name" translatable="yes">Hit Dice</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>